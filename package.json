{
  "name": "boilerplate_edu_souza",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "yarn build && next export",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --bail --findRelatedTests",
    "test:ci": "yarn test --bail --coverage",
    "test:watchFull": "yarn test --bail --coverage --findRelatedTests",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:cleancache": "start-storybook -s ./public -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook -s ./public",
    "generate:component": "yarn plop --plopfile ./generators/component/plopfile.js",
    "commit": "git commit",
    "commit:all": "git add --all && git commit -a",
    "clean-install": "rm -rf node_modules/ && yarn cache clean && yarn install --pure-lockfile"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test:staged"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "next": "11.1.2",
    "next-pwa": "^5.3.1",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.1",
    "styled-icons": "^10.38.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.15.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "prettier": "2.4.0",
    "typescript": "^4.4.2"
  }
}
